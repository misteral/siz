<h1>Тип документа:  <%= @tip_doc.name %>
</h1>


<table class="table table-striped">
  <thead>
    <tr>
      <th>Номер</th>
      <th>Имя</th>
      <th>Создано</th>
      <th>Действия</th>
    </tr>
  </thead>
  <tbody>
<%= render @tip_doc.docs %>
  </tbody>
</table>

<div class="form-actions">
  <%= link_to t('New'), new_doc_path(:tip_docum => @tip_doc.id), :class => 'btn'  %>
</div>

<tr class="nested-fields blabla-fields">

  <td><%= label_tag f.object_name.gsub(/[^0-9]+/,'').to_i+1 %></td>
  <td><%= f.input :siz_id,:as => 'hidden',:wrapper => false %>
    <%= autocomplete_field_tag 'siz_name', (f.object.siz_id) ? Siz.find(f.object.siz_id).name : '' , @autocompurl, :size => 75, :id_element => '#doc_doc_tables_attributes_'+f.object_name.gsub(/[^0-9]+/,'')+'_siz_id' %></td>
  <td><%= label_tag "@tek_siz.nom" %></td>
  <td><%= f.association :ed, :wrapper => :small, :selected => 1, :input_html => { :class =>"siz70" } %></td>
  <td><%= f.association :razmer_od, :wrapper => :small, :input_html => { :class =>"siz70" } %></td>
  <td><%= f.association :rost, :wrapper => :small, :input_html => { :class => "siz80"} %></td>
  <td><%= f.input :kol, :wrapper => :small, :input_html => { :class => "siz70"} %></td>
  <td><%= link_to_remove_association 'Удалить строку', f, :class =>'btn btn-mini'  %></td>
</tr>

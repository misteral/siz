<h1>Docs</h1>
<table class="table table-striped">
  <thead>
    <tr>
      <th>ID</th>
      <th>Name</th>
      <th>Created at</th>
      <th>Actions</th>
    </tr>
  </thead>
  <tbody>
    <% @docs.each do |doc| %>
      <tr>
        <td><%= doc.id %></td>
        <td><%= link_to doc.name, doc_path(doc) %></td>
        <td><%= doc.created_at %></td>
        <td>
          <%= link_to 'Edit', edit_doc_path(doc), :class => 'btn btn-mini' %>
          <%= link_to 'Destroy', doc_path(doc), :method => :delete, :confirm => 'Are you sure?', :class => 'btn btn-mini btn-danger' %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to 'New', new_doc_path, :tip_docum => @tip_docum, :class => 'btn btn-primary' %>

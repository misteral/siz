<%= simple_form_for(@doc) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :name %>
    <%= f.input :utv %>
    <%= f.association :sklad %>
    <%= f.association :postavshik %>
    <%= f.association :rabotnik %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
